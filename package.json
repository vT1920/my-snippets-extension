{
  "name": "snipets-extension",
  "displayName": "My Snippets Extension",
  "description": "A collection of simple snippets  for VS Code",
  "publisher": "vtt",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.79.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vT1920/my-snippets-extension.git"
  },
  "icon": "dragon.png",
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      }
    ]
  }
}